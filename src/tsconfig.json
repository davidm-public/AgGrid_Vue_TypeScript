{
  "compilerOptions": {
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "experimentalDecorators": true,
    "lib": ["es6", "dom"],
    "module": "es2015",
    "moduleResolution": "node",
    "sourceMap": true,
    "strict": false,
    "target": "es5",
    "typeRoots": ["node_modules/@types", "manual_typings"],
    "baseUrl": ".",
    "paths": {
      "lodash/*": ["node_modules/@types/lodash-es/*"]
    }
  },
  "exclude": [
    ".idea",
    ".cache-loader",
    "_old",
    "build",
    "node_modules",
    "resources",
    "static"
  ]
}
